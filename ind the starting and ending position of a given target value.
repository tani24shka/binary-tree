class Solution {
public:
    vector<int> searchRange(vector<int>& nums, int target) {
        int n=nums.size();
        int start=0;
        int end=n-1;
        int low=-1;
        vector<int>v;
        int high=-1;
        while(start<=end)
        {
            int mid=(start+end)/2;
            if(nums[mid]==target)
            {
                low=mid;
                end=mid-1;
            }
            if(nums[mid]>target)
            {
                end=mid-1;
            }
            if(nums[mid]<target)
            {
                start=mid+1;
            }
        }
        v.push_back(low);
         start=0;
        end=n-1;
         while(start<=end)
        {
            int mid=(start+end)/2;
            if(nums[mid]==target)
            {
                high=mid;
                start=mid+1;
            }
            if(nums[mid]>target)
            {
                end=mid-1;
            }
            if(nums[mid]<target)
            {
                start=mid+1;
            }
        }
        v.push_back(high);
        return v;
    }
};
